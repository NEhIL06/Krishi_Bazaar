{
  "expo": {
    "name": "Krishibazar",
    "slug": "krishibazar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "krishibazar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A7C59"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krishibazar.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4A7C59"
      },
      "package": "com.krishibazar.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "krishibazar",
          "organization": "nehil"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
